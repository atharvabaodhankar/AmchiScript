chala suru karu;

heAhe num = 7;
heAhe flag = khara;

// Logical AND
jar ((num > 0) ani (num < 10)) {
  dakhava num, " is between 1 and 9.";
}

// Logical OR
jar ((num < 0) kimva (num > 5)) {
  dakhava num, " is less than 0 or greater than 5.";
}

// Logical NOT
jar (nahi flag) {
  dakhava "Flag is false.";
} nahitar {
  dakhava "Flag is true.";
}

bas re ata; 